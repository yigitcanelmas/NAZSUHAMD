# Production Environment Configuration
VITE_APP_ENV=production
VITE_APP_NAME=Nazsu Water Store
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=20000
VITE_API_RETRY_ATTEMPTS=5

# Database Configuration
VITE_DB_HOST=prod-db.nazsu.com
VITE_DB_PORT=27017
VITE_DB_NAME=nazsu_production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_HOT_RELOAD=false

# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_ANALYZE=false



# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id
VITE_GOOGLE_REDIRECT_URI=https://nazsu.com/auth/callback

# Payment Gateway (Live)
VITE_PAYMENT_GATEWAY_URL=https://payment.example.com
VITE_PAYMENT_PUBLIC_KEY=live_pk_123456789

# Logging
VITE_LOG_LEVEL=error
VITE_LOG_TO_CONSOLE=false
VITE_LOG_TO_FILE=true

# CDN Configuration
VITE_CDN_URL=https://cdn.nazsu.com
VITE_ASSETS_URL=https://assets.nazsu.com

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_STRATEGY=cacheFirst

# Monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# SEO
VITE_SITE_URL=https://nazsu.com
VITE_SITE_DESCRIPTION=Nazsu Su - Kaliteli damacana su hizmeti
VITE_SITE_KEYWORDS=damacana su, su sipari≈üi, nazsu, temiz su
